<div class="FCKThumb">
  <table border="0" cellpadding="0" cellspacing="0">
    <tbody>
      <tr>
        <td align="center" valign="middle">
          <% @images.each do |image| %>
            <td>
	          <tr>
                <%= link_to(image_tag(thumb_admin_image_path(image, :png)),
 					{ :controller => "ckeditor",
	 				  :action => "select_image", :id => image.id})
	 			%>
              </tr>
            </td>
          <% end %>
        </td>
        <%= page_entries_info @images %>
   	    <%= will_paginate @images %>
      </tr>
    </tbody>
  </table>
</div>